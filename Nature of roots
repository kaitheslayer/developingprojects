from math import sqrt
def roots(a, b, c):
    root = b**2 - 4*a*c
    if root < 0:
        return "Unreal roots"
    elif sqrt(root) == sqrt(root)//1:  #if perfect square
        return "Two rational roots"
    elif root > 0:
        return "Two irrational roots"
    elif root == 0:
        return "Two equal roots"


print(roots(1,5,4))

