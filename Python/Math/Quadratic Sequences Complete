def qseq (qs1, qs2, qs3, qs4, qs5):
    if (qs5 - qs4) - (qs4 - qs3) == (qs3 - qs2) - (qs2 - qs1):
        a = (qs3 - qs2) - (qs2 - qs1) / 2  # a = 2nd difference/2
        b = (qs2 - qs1) - 3 * a # b = (T2 - T1)-3a
        c = qs1 - (a - b)  # c = T1-(a+b)
        if a == 1:
             a = ""
        return "Tn = %snÂ² + %sn + %s" % ( a, b, c)
    else:
        print("This is not quadratic!")
