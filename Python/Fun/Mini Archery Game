username = ""
word_limit = "yes"
while word_limit == "yes":
    username = input("Username: (max 25 characters)  ")
    if username == "admin//Fried-Liver//":
        lvl = input("Enter Level: ")
        level = int(lvl)
    if len(username)<=25:
        word_limit = "no"
        show_rules = input("Do you know how to play? (y/n)")
        if show_rules == "n":
            print("* PRESS 's' THEN 'Enter' TO OPEN SHOP *")
            tutorial_game = input("| Shop(s) | Start Game(L>) | ")
            if tutorial_game == "s":
                print(tutorial_shop)
                print("\n* PRESS 'Enter' TO START GAME, THEN PRESS 'Enter' AGAIN TO FIRE AN ARROW *\n")
                input ("Start Game")
                input("Fire!")
                score = [5, 5, 5, 5, 5, 10, 10, 10, 10, 20, 20, 20, 50]
                print("\n* YOUR SCORE IS *\n")
                print("Score: %s" % (random.choice(score)))
                xp += (random.randint(1,12))
                print("XP:" + str(xp))
                print("\n* FROM THIS ROUND GAINED 'xp', FOR EVERY 25 XP YOU EARN, YOU LEVEL UP *\n ")
                print("* TUTORIAL COMPLETED *\n")
            else:
                pass



spacing = " "*(146-(len(username)))


